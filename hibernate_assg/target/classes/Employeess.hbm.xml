<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="hibernate_assg.Employeess" table="Employees">
<id name="id" column="id">
<generator class="identity"/>
</id>
<property name="name" column="name" not-null="true" />
<property name="phon" column="phon" not-null="true" unique="true"/>
<property name="email" column="email" not-null="true" unique="true"/>
<property name="password" column="password" not-null="true" />
<property name="designation" column="designation" not-null="true"/>
<property name="salary" column="salary" not-null="true"/>


</class>
</hibernate-mapping>